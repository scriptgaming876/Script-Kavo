local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/ZeianRussell/Kavo-UI-Library/main/Movable.source.lua"))()
local Window = Library.CreateLib("Dead Rails Hubs", "DarkTheme")
local Tab = Window:NewTab("Player")
local Section = Tab:NewSection("Player Hubs")
Section:NewSlider("WalkSpeed", "Walkspeed", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
if humanoid then
        humanoid.WalkSpeed = s
    end
end)
end)

Section:NewButton("Noclip", "Wall Hack", function()
    print("Executed Noclip")
loadstring(game:HttpGet("https://raw.githubusercontent.com/scriptgaming876/Script-Kavo/refs/heads/main/DeadRails/Noclip"))()
end)

