local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/ZeianRussell/Kavo-UI-Library/main/Movable.source.lua"))()
local Window = Library.CreateLib("Noob Indo Hub-Bisa Di Banned Roblox", "BloodTheme")
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Main Player")
Section:NewToggle("Auto Hide", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/scriptgaming876/Script-Kavo/refs/heads/main/Doors/Script/Auto%20Hide.lua"))()
    else
        print("Toggle Off")
    end
end)

Section:NewToggle("Auto Loot", "Premium", function(state)
    if state then
        print("Toggle On")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/scriptgaming876/Script-Kavo/refs/heads/main/Doors/Script/Premium%20Not%20Open/Auto%20Loot"))()
    else
        print("Toggle Off")
local StarterGui = game:GetService("StarterGui")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local achievementEvent = ReplicatedStorage:FindFirstChild("AchievementUnlock")
if achievementEvent and achievementEvent:IsA("RemoteEvent") then
    achievementEvent.OnClientEvent:Connect(function(achievementName)
        StarterGui:SetCore("SendNotification", {
            Title = "Buy Premium Toggle Auto Loot,
            Text = tostring(achievementName),
            Duration = 4
        })
    end)
end
    end
end)

Section:NewToggle("Noclip", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/scriptgaming876/Script-Kavo/refs/heads/main/Doors/Script/Noclip"))()
    else
        print("Toggle Off")
    end
end)

Section:NewKeybind("Noclip", "KeybindInfo", Enum.KeyCode.N, function()
	print("Executed Noclip")
loadstring(game:HttpGet("https://raw.githubusercontent.com/scriptgaming876/Script-Kavo/refs/heads/main/Doors/Script/Noclip"))()
end)

Section:NewToggle("God Mode", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
